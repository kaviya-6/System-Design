class Keyboard {
    public void input() {
        System.out.println("Typing...");
    }
}

class Computer {
    private Keyboard keyboard;

    public Computer() {
        this.keyboard = new Keyboard(); // tightly dependent
    }

    public void use() {
        keyboard.input();
    }
}
public class Main {
    public static void main(String[] args) {
        Computer computer = new Computer();
        computer.use();
    }
}
